{
  "name": "tdd-react-starter",
  "description": "This is a starter package containing react, typescript, babel, cypress, jest,enzyme, prettier and tailwind. It is bundled and served with parcel.",
  "version": "1.0.0",
  "main": "index.js",
  "author": "Rumit Rout <rumitr@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.0.0-0",
    "@babel/plugin-proposal-class-properties": "^7.13.0",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/preset-env": "^7.13.10",
    "@babel/preset-react": "^7.12.13",
    "@tailwindcss/postcss7-compat": "^2.0.4",
    "@types/react": "^17.0.3",
    "@types/react-dom": "^17.0.3",
    "autoprefixer": "^9",
    "babel-core": "^7.0.0-0",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^26.6.3",
    "cypress": "^6.6.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.6",
    "eslint": "^7.22.0",
    "eslint-config-codingitwrong": "^0.1.4",
    "eslint-plugin-cypress": "^2.11.2",
    "eslint-plugin-jest": "^24.3.1",
    "jest": "^26.6.3",
    "parcel-bundler": "1.12.3",
    "postcss": "^7",
    "tailwindcss": "npm:@tailwindcss/postcss7-compat",
    "typescript": "^4.2.3"
